      <Label layoutX="8.0" layoutY="8.0" text="Configuración Maestra">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
      <Button fx:id="emptyDatabase" layoutX="162.0" layoutY="236.0" mnemonicParsing="false" onAction="#emptyPressed" text="Vaciar Base De Datos" />
      <Button layoutX="28.0" layoutY="93.0" mnemonicParsing="false" onAction="#lostPasswordPressed" text="Perdí mi contraseña" />
      <Label layoutX="14.0" layoutY="202.0" text="ADVERTENCIA: Perdida de Datos Posible">
         <font>
            <Font size="21.0" />
         </font>
      </Label>
      <Button fx:id="configurationAssistant" layoutX="28.0" layoutY="56.0" mnemonicParsing="false" onAction="#configurationAssistantPressed" text="Asistente de Configuración" />
   </children>
</AnchorPane>
